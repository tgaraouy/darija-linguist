{"ast":null,"code":"import { defineComponent, ref, computed, onMounted, watch } from 'vue';\nimport { useWords } from '@/composables/useWords';\nimport { useAudio } from '@/composables/useAudio';\nimport WordDisplay from './WordDisplay.vue';\nimport WordDetails from './WordDetails.vue';\nexport default defineComponent({\n  name: 'DarijaLinguist',\n  components: {\n    WordDisplay,\n    WordDetails\n  },\n  setup() {\n    const {\n      words,\n      fetchWords\n    } = useWords();\n    const {\n      speak\n    } = useAudio();\n    const currentIndex = ref(0);\n    const currentWord = computed(() => words.value[currentIndex.value]);\n    const nextWord = () => {\n      currentIndex.value = (currentIndex.value + 1) % words.value.length;\n    };\n    const speakWord = text => {\n      speak(text);\n    };\n    onMounted(async () => {\n      await fetchWords();\n      console.log('Words fetched:', words.value);\n    });\n    watch(words, newWords => {\n      console.log('Words updated:', newWords);\n    });\n    return {\n      words,\n      currentWord,\n      nextWord,\n      speakWord\n    };\n  }\n});","map":{"version":3,"names":["defineComponent","ref","computed","onMounted","watch","useWords","useAudio","WordDisplay","WordDetails","name","components","setup","words","fetchWords","speak","currentIndex","currentWord","value","nextWord","length","speakWord","text","console","log","newWords"],"sources":["C:\\darija-linguist\\frontend\\src\\components\\DarijaLinguist.vue"],"sourcesContent":["<template>\r\n    <v-container>\r\n      <h1 class=\"text-h3 mb-5 text-center\">Darija Linguist</h1>\r\n      <div>Debug: {{ words.length }} words loaded</div>\r\n      <div v-if=\"currentWord\">\r\n        Current Word: {{ currentWord.darija }}\r\n      </div>\r\n      <div v-else>\r\n        No current word\r\n      </div>\r\n      <v-row v-if=\"currentWord\">\r\n        <v-col cols=\"12\" md=\"6\">\r\n          <WordDisplay :word=\"currentWord\" @speak-word=\"speakWord\" />\r\n        </v-col>\r\n        <v-col cols=\"12\" md=\"6\">\r\n          <WordDetails :word=\"currentWord\" />\r\n        </v-col>\r\n      </v-row>\r\n      <v-row class=\"mt-5\">\r\n        <v-col class=\"text-center\">\r\n          <v-btn color=\"primary\" @click=\"nextWord\">Next Word</v-btn>\r\n        </v-col>\r\n      </v-row>\r\n    </v-container>\r\n  </template>\r\n  \r\n  <script>\r\n  import { defineComponent, ref, computed, onMounted, watch } from 'vue'\r\n  import { useWords } from '@/composables/useWords'\r\n  import { useAudio } from '@/composables/useAudio'\r\n  import WordDisplay from './WordDisplay.vue'\r\n  import WordDetails from './WordDetails.vue'\r\n  \r\n  export default defineComponent({\r\n    name: 'DarijaLinguist',\r\n    components: {\r\n      WordDisplay,\r\n      WordDetails,\r\n    },\r\n    setup() {\r\n      const { words, fetchWords } = useWords()\r\n      const { speak } = useAudio()\r\n      const currentIndex = ref(0)\r\n  \r\n      const currentWord = computed(() => words.value[currentIndex.value])\r\n  \r\n      const nextWord = () => {\r\n        currentIndex.value = (currentIndex.value + 1) % words.value.length\r\n      }\r\n  \r\n      const speakWord = (text) => {\r\n        speak(text)\r\n      }\r\n  \r\n      onMounted(async () => {\r\n        await fetchWords()\r\n        console.log('Words fetched:', words.value)\r\n      })\r\n  \r\n      watch(words, (newWords) => {\r\n        console.log('Words updated:', newWords)\r\n      })\r\n  \r\n      return {\r\n        words,\r\n        currentWord,\r\n        nextWord,\r\n        speakWord,\r\n      }\r\n    },\r\n  })\r\n  </script>"],"mappings":"AA2BE,SAASA,eAAe,EAAEC,GAAG,EAAEC,QAAQ,EAAEC,SAAS,EAAEC,KAAI,QAAS,KAAI;AACrE,SAASC,QAAO,QAAS,wBAAuB;AAChD,SAASC,QAAO,QAAS,wBAAuB;AAChD,OAAOC,WAAU,MAAO,mBAAkB;AAC1C,OAAOC,WAAU,MAAO,mBAAkB;AAE1C,eAAeR,eAAe,CAAC;EAC7BS,IAAI,EAAE,gBAAgB;EACtBC,UAAU,EAAE;IACVH,WAAW;IACXC;EACF,CAAC;EACDG,KAAKA,CAAA,EAAG;IACN,MAAM;MAAEC,KAAK;MAAEC;IAAW,I